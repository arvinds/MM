<div id='selection_box' class="whitebox">
	<h3>Want to go somewhere?</h1>
	<div id="to_from_container">
		<div class="from">
			<div class="input-label">From</div><input id='from_select' type="text" val="" />
		</div>
		<div class="to">
			<div class="input-label">To</div><input id='to_select' type="text" val="" />
		</div>
		<div class="when">
			<div class="input-label">When</div><input id='when_select' type="text" val="" />
		</div>
		<div class="action-container">
			<button id="find_ride" class="orange-button">Find a Ride</button>
			<button id="post_ride" class="blue-button">Post a Ride</button>
			<div style="clear:both"></div>
		</div>
	</div>
</div>
<div id='map_container' class="hidden">
</div>
<div style="clear:both"></div>
<div id='subpage' class='whitebox fnone'>
	<div class="subpage-loading">
		<div class="loadbar" alt="loading"></div>
	</div>
	<div class="subpage-content hidden">
	</div>
</div>
